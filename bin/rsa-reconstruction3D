#!/bin/sh

################################################################################
##
## Start running Rootwork (3D Reconstruction) program per image set without cropping 
##
## Fri May 31 2013 Vladimir Popov <vladimir.popov@duke.edu> 
##
################################################################################

currdir=`dirname $0`

reconstruction3D=$currdir/matlab-programs/Reconstruction3D

echo "*************************************************************************************"
echo "* Starting Reconstruction3D program from $reconstruction3D/start"
echo "*************************************************************************************"

$reconstruction3D/start $@
