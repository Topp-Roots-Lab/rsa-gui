#!/usr/bin/env xdg-open
[Desktop Entry]
Version=4.0.0
Name=RSA-GiA
GenericName=Root System Architecture, General Image Analysis
Comment=Analyze root system architecture
Type=Application
Exec=bash -c "/opt/rsa-gia/bin/rsa-gia 2>&1 | tee -a /var/log/rsa-gia/\$USER.\$(expr \$(find /var/log/rsa-gia/ -type f -iname "\$USER.*.log" | wc -l) + 1).log"
Icon=/usr/share/pixmaps/rsa-gia.png
Terminal=false
Categories=Application;
