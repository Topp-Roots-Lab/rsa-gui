<?xml version="1.0" encoding="UTF-8"?>
<params>
  <do-crop>0</do-crop>
  <use-giaroots-crop>0</use-giaroots-crop>
  <do-likelihood-img>1</do-likelihood-img>
  <do-silhouette-hysteresis>1</do-silhouette-hysteresis>
  <use-giaroots-thresholding>0</use-giaroots-thresholding>
  <do-silhouette-harmonic>0</do-silhouette-harmonic>
  <do-axis>1</do-axis>
  <do-reconstruct-hysteresis>1</do-reconstruct-hysteresis>
  <extension-root-images>jpg</extension-root-images>
  <extension-giaroots-crop-images>jpg</extension-giaroots-crop-images>
  <extension-giaroots-thresh-images>jpg</extension-giaroots-thresh-images>
